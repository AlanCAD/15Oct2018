//ellipse(100,400,150,150);
//ellipse(400,100,150,150);
//ellipse(400,400,150,150);

void setup() {
size(500,500);
}

int x1= 100;
int y1=100;
int x2=100;
int y2=400;
int x3=400;
int y3=100;
int x4=400;
int y4=400;


void draw() {
	background(100);
ellipse(x1,y1,150,150);
ellipse(x2,y2,150,150);
ellipse(x3,y3,150,150);
ellipse(x4,y4,150,150);
	
x1++;
y1++;
x2++;
y2--;
x3--;
y3++;
x4--;
y4--;
		
}
